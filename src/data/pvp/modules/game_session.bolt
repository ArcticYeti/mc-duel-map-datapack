from pvp:maps import initialize_map, reset_map
from pvp:kits import give_kit

def start_game_session(map_id: str):
	# TODO: make this not just target all players, account for spectators as well
	as @a:
		give_kit('BASIC')


	initialize_map(map_id)

def end_game_session(map_id: str):
	reset_map(map_id)



